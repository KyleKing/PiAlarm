{
  "source": "./modules",
  "destination": "./docs",
  "includes": ["\\.(js|jsx|es\\d)$"],
  "plugins": [
  	{
	  	"name": "esdoc-jsx-plugin",
	  	"option": {"enable": true}
	  },
	  {
	  	"name": "esdoc-standard-plugin",
	    "option": {
	      "undocumentIdentifier": { "enable": true },
	      "unexportedIdentifier": { "enable": true }
	    }
	  }
	]
}
